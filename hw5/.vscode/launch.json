{
"version": "0.2.0",
"configurations": [
                {
                "name": "Run-hw5",
                "type": "cppdbg",
                "request": "launch",
                "program": "${workspaceFolder}/hw5",
                "args": [
                        /*filename:*/      "image/fs.img",
                        /*command:*/       "cp",
                        /*src:*/           "usertests",
                        /*dst:*/           "usertests_copy"
                        ],
                "stopAtEntry": false,
                "cwd": "${workspaceFolder}",
                "environment": [],
                "externalConsole": false,
                "MIMode": "gdb",
                "miDebuggerPath": "/usr/bin/gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
                        "ignoreFailures": true
                    }],
                "preLaunchTask": "make"
            }
        ]
}
