{
"version": "0.2.0",
"configurations": [
                {
                "name": "Run-hw2",
                "type": "cppdbg",
                "request": "launch",
                "program": "${workspaceFolder}/bin/hw2",
                "args": [
                        /*filename:*/      "output.txt", 
                        /*num_threads:*/   "100",
                        /*num_counters:*/  "100",
                        /*log_enabled:*/   "1"
                        ],
                "stopAtEntry": false,
                "cwd": "${workspaceFolder}",
                "environment": [],
                "externalConsole": false,
                "MIMode": "gdb",
                "miDebuggerPath": "/usr/bin/gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
                        "ignoreFailures": true
                    }],
                "preLaunchTask": "make"
            }
        ]
}
